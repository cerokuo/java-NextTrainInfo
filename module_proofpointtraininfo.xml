      <src refid="proofpointtraininfo.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.proofpointtraininfo"/>
    </javac2>
    
    <copy todir="${proofpointtraininfo.testoutput.dir}">
      <fileset dir="${module.proofpointtraininfo.basedir}/src/test/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="clean.module.proofpointtraininfo" description="cleanup module">
    <delete dir="${proofpointtraininfo.output.dir}"/>
    <delete dir="${proofpointtraininfo.testoutput.dir}"/>

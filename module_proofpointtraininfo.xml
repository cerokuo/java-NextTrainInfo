  </patternset>
  
  <path id="proofpointtraininfo.module.sourcepath">
    <dirset dir="${module.proofpointtraininfo.basedir}">
      <include name="src/main/java"/>
      <include name="src/main/resources"/>
    </dirset>
  </path>
  
  <path id="proofpointtraininfo.module.test.sourcepath">
    <dirset dir="${module.proofpointtraininfo.basedir}">
      <include name="src/test/java"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.proofpointtraininfo" depends="compile.module.proofpointtraininfo.production,compile.module.proofpointtraininfo.tests" description="Compile module ProofPointTrainInfo"/>
  
  <target name="compile.module.proofpointtraininfo.production" depends="register.custom.compilers" description="Compile module ProofPointTrainInfo; production classes">
    <mkdir dir="${proofpointtraininfo.output.dir}"/>
    <javac2 destdir="${proofpointtraininfo.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.proofpointtraininfo}/javac">
      <compilerarg line="${compiler.args.proofpointtraininfo}"/>
      <bootclasspath refid="proofpointtraininfo.module.bootclasspath"/>
      <classpath refid="proofpointtraininfo.module.production.classpath"/>
      <src refid="proofpointtraininfo.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.proofpointtraininfo"/>
    </javac2>
    
    <copy todir="${proofpointtraininfo.output.dir}">
      <fileset dir="${module.proofpointtraininfo.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.proofpointtraininfo.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.proofpointtraininfo.tests" depends="register.custom.compilers,compile.module.proofpointtraininfo.production" description="compile module ProofPointTrainInfo; test classes" unless="skip.tests">
    <mkdir dir="${proofpointtraininfo.testoutput.dir}"/>
    <javac2 destdir="${proofpointtraininfo.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.proofpointtraininfo}/javac">
